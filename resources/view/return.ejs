<!DOCTYPE html>
<html lang="en">
  <%- include('common/header') %> 
  <%- include('common/nav') %> 
<!-- end mobile menu -->
  
  <div class="breadcrumbs">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <ul>
            <li class="home"> <a title="Go to Home Page" href="/">Home</a><span>&raquo;</span></li>
           
            <li><strong>FAQ</strong></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <!-- Breadcrumbs End -->
  <div class="container">
      <div class="row">
        <div class="faq-page">
          <div class="col-xs-12">
            <div class="page-title">
              <h2>Return Policy</h2>
              
              <p>Get the details of return Policy.</p>
            </div></div>
            <div class="col-xs-12 col-sm-12">
            <div class="panel-group accordion-faq" id="faq-accordion">
              <% if (returnpolicy) { %>
               <% returnpolicy.forEach(qa => { %>
                <div class="panel">
                  <div class="panel-heading"> <a data-toggle="collapse" data-parent="#faq-accordion" href="#question1"> <span class="arrow-down"><i class="fa fa-angle-down"></i></span></span> <span class="arrow-up"><i class="fa fa-angle-up"></i></span></span><%= qa.title %> . </a> </div>
                  <div id="question1" class="panel-collapse collapse in">
                    <div class="panel-body"> <%= qa.return %><br><br></div>
                  </div>
                </div>
               <% }) %>
              <% }else{ %>
             <p>No Question and Answer Yet.......</p>
              <%}%>
             
            </div>
          </div>
        </div>
      </div>
  </div>

  <!-- service section -->
  <div class="jtv-service-area">
