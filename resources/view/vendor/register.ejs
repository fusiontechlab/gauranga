.login_copylist{
	margin-bottom: 0px;
	margin-top: 20px;
	font-size: 14px;
	font-weight: 500;
	text-align: right;
    color: #777;
}
.login_copylist li{
	display: inline-block;
	list-style: none;
	margin-left: 15px;
	position: relative;
}
.login_copylist li:before
{
	content: "";
    position: absolute;
    width: 100%;
    height: 2px;
    bottom: -5px;
    opacity: 1;
    left: 0;
    background-color: #333;
    visibility: hidden;
    -webkit-transform: scaleX(0);
    transform: scaleX(0);
    -webkit-transition: all 0.3s ease-in-out 0s;
    transition: all 0.3s ease-in-out 0s;
}
.login_copylist li:hover:before{
	visibility: visible;
    -webkit-transform: scaleX(1);
    transform: scaleX(1);
}
.login_copylist li a{
	color: #777;
}
.login_copylist li a:hover{
	text-decoration: none;
}
.register .back_btn a:hover{
	text-decoration: none;
}
